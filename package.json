{
  "name": "walmart-items",
  "version": "1.0.0",
  "description": "Mullet Stack: Facebook in the front. Walmart in the back. (React.js/Hapi)",
  "main": "walmart.js",
  "watch": {
    "test": "{src,test}/*.js"
  },
  "scripts": {
    "webpack": "webpack",
    "test": "jest --verbose && lab -v -l",
    "lint": "eslint . --ext .js",
    "start": "nodemon --watch src babelify.js src/server/server.js",
    "format": "prettier --single-quote --write *.js src/components/*.js test/*.js"
  },
  "dependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.4.0",
    "babel-jest": "^21.0.0",
    "babel-loader": "^7.0.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-register": "^6.26.0",
    "blipp": "^3.0.0",
    "body-parser": "^1.14.1",
    "bootstrap": "^3.3.5",
    "h2o2": "^7.0.0",
    "hapi": "^17.0.0",
    "inert": "^5.1.0",
    "jest": "^21.0.0",
    "npm-watch": "^0.2.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-proptypes": "^1.0.0",
    "react-scripts": "^1.0.0",
    "vision": "^5.3.3",
    "webpack": "^3.0.0"
  },
  "devDependencies": {
    "code": "^5.0.0",
    "enzyme": "^2.7.1",
    "eslint": "^4.19.1",
    "eslint-config-eta": "^1.0.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-react": "^7.10.0",
    "jest-cli": "^21.0.0",
    "lab": "^14.0.0",
    "prettier": "^1.0.1",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^15.4.2"
  }
}
